<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:focusable="true"
    android:focusableInTouchMode="true"
    android:orientation="vertical" >

    <TextView
        android:id="@+id/title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="5dp"
        android:layout_centerHorizontal="true"
        android:ellipsize="end"
        android:singleLine="true"
        android:gravity="center_horizontal"
        android:text="@string/telefones_dialog_title"
        style="@style/DialogText.Title"/>

    <ListView
        android:id="@+id/telefones_list"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:divider="#ffd8d2d2"
        android:dividerHeight="1dp"
        android:headerDividersEnabled="false"
        android:footerDividersEnabled="true"
        style="@style/ListView"
        android:layout_below="@id/title"/>

</RelativeLayout><!-- From: file:/F:/Projetos/Android/TCC/TCC_Android/MenusProject/Menus/src/main/res/layout/dialog_telefones.xml -->