<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.android.menus"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="7"
        android:targetSdkVersion="18" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="Menus"
        android:theme="@style/Theme.Style1" >
        <activity
            android:name="br.com.android.menus.activity.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="br.com.android.menus.activity.RamosFragmentActivity"
            android:label="@string/title_activity_ramo" >
        </activity>
        <activity
            android:name="br.com.android.menus.activity.EstabelecimentosFragmentActivity"
            android:label="@string/title_activity_estabelecimentos_fragment" >
        </activity>
        <activity
            android:name="br.com.android.menus.activity.EstabelecimentosFavoritosFragmentActivity"
            android:label="@string/title_activity_estabelecimentos_favoritos_fragment" >
        </activity>
        <activity
            android:name=".activity.ProdutosFragmentsActivity"
            android:label="@string/title_activity_produtos_fragments" >
        </activity>
        <activity
                android:name=".activity.AppFragmentActivity"
                android:label="@string/title_activity_produtos_fragments" >
        </activity>
    </application>

</manifest>
